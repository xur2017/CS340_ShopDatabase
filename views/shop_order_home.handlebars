<h2>Shop_order</h2>

<form id="addForm">
    userId:
    <input type="number" name="userId" id="userIdInput"><br>
    orderDate:
    <input type="date" name="orderDate" id="orderDateInput"><br>
    <input type="submit" name="add" value = "Add" id="addButton">
</form>

<br>

<table id="showTable">
    <tr>
        <th>userId</th>
        <th>orderDate</th>
    </tr>
    
    {{#if results}}
        {{#each results}}
            <tr>
                <td>{{this.userId}}</td>
                <td>{{this.orderDate}}</td>
            </tr>
        {{/each}}
    {{/if}}
</table>

<script src="./shop_order_script.js"></script>